<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Group Execution" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.sevenrmartsupermarket.listners.ExtentReporter"></listener>
	</listeners>
	<test name="Sanity test Run">
		<groups>
			<run>
				<include name="Sanity Test">
				</include>
			</run>
		</groups>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.AdminUserTest"></class>
			<class name="com.sevenrmartsupermarket.tests.PushNotificationsTest"></class>
		</classes>
	</test>
</suite>