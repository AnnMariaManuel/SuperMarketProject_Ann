<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Health check Execution" thread-count="5">
	<listeners>
		<listener class-name="com.sevenrmartsupermarket.listners.ExtentReporter"></listener>
	</listeners>

	<test name="Health check Login">
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
				<methods>
					<include name="verifyLoginFunctionality"></include>
				</methods>
			</class>

		</classes>
	</test>

	<test name="Health check AdminUser">
		<classes>

			<class name="com.sevenrmartsupermarket.tests.AdminUserTest">
				<methods>
					<include name="verifyAdminUserCreation"></include>
				</methods>
			</class>

		</classes>
	</test>


</suite>



